{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "${EXPO_PUBLIC_BASE_URL}",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "${EXPO_PUBLIC_GEOAPIFY_API_KEY}",
        "EXPO_PUBLIC_GOOGLE_API_KEY": "${EXPO_PUBLIC_GOOGLE_API_KEY}"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "${EXPO_PUBLIC_BASE_URL}",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "${EXPO_PUBLIC_GEOAPIFY_API_KEY}",
        "EXPO_PUBLIC_GOOGLE_API_KEY": "${EXPO_PUBLIC_GOOGLE_API_KEY}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BASE_URL": "${EXPO_PUBLIC_BASE_URL}",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "${EXPO_PUBLIC_GEOAPIFY_API_KEY}",
        "EXPO_PUBLIC_GOOGLE_API_KEY": "${EXPO_PUBLIC_GOOGLE_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}